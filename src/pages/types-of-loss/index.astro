---
import CategoryPageHeader from "@ui/composites/CategoryPageHeader.astro";
import SitePageLayout from "@ui/layouts/SitePageLayout.astro";
import Container from "@ui/primitives/Container.astro";
import Typography from "@ui/primitives/Typography.astro";
import { getEntry } from "astro:content";

const categoryEntry = await getEntry(
  "categories",
  "95e45462-a8a7-48cb-882d-407422534681",
);
if (!categoryEntry) {
  throw new Error("Could not get [categoryEntry] in [types-of-loss]");
}
---

<SitePageLayout>
  <CategoryPageHeader categoryEntryData={categoryEntry.data} />
  <Container>
    <Typography set:text="Coming Soon..." variant="body-2" />
  </Container>
</SitePageLayout>
